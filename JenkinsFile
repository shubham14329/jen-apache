pipeline{
    agent any 
    
    stages{
        stage("CLoning"){
            steps{
                sh """
		sudo git https://github.com/shubham14329/jen-apache.git
                """
            }
        }
	stage("Copying to apache"){
	    steps{
	    	sh"""
		sudo cp ./* /var/www/html/ 
		"""
	    }
	}
    }
}
